[Unit]
Description=Prometheus
After=network.target

[Service]
User=prometheus
Group=prometheus
Type=simple
ExecStart=/etc/prometheus/prometheus-2.43.0.linux-amd64/prometheus --config.file=/etc/prometheus/prometheus-2.43.0.linux-amd64/prometheusconf.yml
Restart=on-failure

[Install]
WantedBy=multi-user.target
